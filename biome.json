{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"style": {
				"noDefaultExport": "warn",
				"noVar": "error"
			}
		}
	},
	"javascript": {
		"formatter": {
			"indentStyle": "tab",
			"semicolons": "asNeeded"
		}
	},
	"organizeImports": {
		"enabled": true
	}
}
